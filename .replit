modules = ["nodejs-20", "web", "python-3.11", "postgresql-16"]
[agent]
expertMode = true
integrations = ["youtube:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["redis", "yt-dlp", "libuuid"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Dev Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Worker"

[[workflows.workflow]]
name = "Next.js Dev Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Worker"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "bash -c \"export PERSON_CONF=0.15; redis-server --daemonize yes --port 6379 2>/dev/null || true; npm run worker\""

[[workflows.workflow]]
name = "tiktok worker"
mode = "sequential"
author = 48409791

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run worker:tiktok"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 6379
externalPort = 3000

[[ports]]
localPort = 36393
externalPort = 3001

[[ports]]
localPort = 39369
externalPort = 3002
